    "# Remove weird words\n",
    "remove_words_2 = ['link','matic', 'vet', 'xlm', 'vra', 'low', 'back', 'chart', 'bullish', 'next', 'high', 'qnt','tel', 'eos', 'defi', 'bsc', 'hex', 'dogefam', 'dogefam_dogehouse', 'dogehouse', 'ripple', 'ray', 'shiba', 'dont', 'know', 'cryptoart']\n",
