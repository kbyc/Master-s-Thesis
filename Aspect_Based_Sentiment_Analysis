    "### Replace contractions with the full form"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 77,
   "id": "c8917a49",
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "<ipython-input-77-77f53f041f0f>:8: SettingWithCopyWarning: \n",
      "A value is trying to be set on a copy of a slice from a DataFrame.\n",
      "Try using .loc[row_indexer,col_indexer] = value instead\n",
      "\n",
      "See the caveats in the documentation: https://pandas.pydata.org/pandas-docs/stable/user_guide/indexing.html#returning-a-view-versus-a-copy\n",
      "  df_asp['content_contr'] = df_asp['content_url'].apply(lambda text: expandContractions(text))\n"
     ]
    }
   ],
   "source": [
    "c_re = re.compile('(%s)' % '|'.join(cList.keys()))\n",
    " \n",
    "def expandContractions(text, c_re=c_re):\n",
    "    def replace(match):\n",
    "        return cList[match.group(0)]\n",
    "    return c_re.sub(replace, text.lower())\n",
    " \n",
    "df_asp['content_contr'] = df_asp['content_url'].apply(lambda text: expandContractions(text))\n",
    " "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 78,
   "id": "2c4790bb",
   "metadata": {},
   "outputs": [
