    "### Remove URL"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "id": "221eb212",
   "metadata": {},
   "outputs": [],
   "source": [
    "def remove_urls(text):\n",
    "    url_pattern = re.compile(r'https?://\\S+|www\\.\\S+')\n",
    "    return url_pattern.sub(r'', text)\n",
    "\n",
    "df_final[\"content_url\"] = df_final[\"content\"].apply(lambda text: remove_urls(text))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
