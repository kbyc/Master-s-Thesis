   ],
   "source": [
    "from collections import Counter\n",
    "cnt = Counter()\n",
    "for text in df_final[\"final_tweets\"].values:\n",
    "    for word in text.split():\n",
    "        cnt[word] += 1\n",
    "        \n",
    "cnt.most_common(15)\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "c027b914",
   "metadata": {},
   "source": [
    "### Remove the most frequent words"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 69,
   "id": "c83104d0",
   "metadata": {},
   "outputs": [],
   "source": [
    "FREQWORDS = set([w for (w, wc) in cnt.most_common(15)])\n",
    "def remove_freqwords(text):\n",
    "    \"\"\"custom function to remove the frequent words\"\"\"\n",
    "    return \" \".join([word for word in str(text).split() if word not in FREQWORDS])\n",
    " \n",
    "df_final[\"content_freq\"] = df_final[\"final_tweets\"].apply(lambda text: remove_freqwords(text))\n"
   ]
  },
